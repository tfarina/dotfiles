worker_processes  1;

events {
    worker_connections  1024;
}

http {
    include       mime.types;
    default_type  application/octet-stream;

    sendfile        on;
    keepalive_timeout  65;
    gzip  on;

    #http://www.rackspace.com/knowledge_center/article/ubuntu-and-debian-nginx-from-source-layout
    #sudo ln -s /usr/local/nginx/sites-available/tfarina.org /usr/local/nginx/sites-enabled/tfarina.org
    include /usr/local/nginx/sites-enabled/*;
}
